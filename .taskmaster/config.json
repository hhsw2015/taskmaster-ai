    "enableProxy": false,
    "anonymousTelemetry": true,
    "userId": "1234567890",
    "azureBaseURL": "https://your-endpoint.azure.com/",
    "defaultTag": "master"
  },
  "storage": {
    "type": "local"
  },
  "claudeCode": {},
  "codexCli": {
    "approvalMode": "never",
    "sandboxMode": "danger-full-access",
    "fullAuto": true,
    "skipGitRepoCheck": true,
    "reasoningEffort": "xhigh"
  },
  "grokCli": {
    "timeout": 120000,
    "workingDirectory": null,
    "defaultModel": "grok-4-latest"
  }
